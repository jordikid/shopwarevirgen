{
    "name": "sroze/messenger-enqueue-transport",
    "type": "symfony-bundle",
    "description": "Enqueue adapter for Symfony Messenger component",
    "keywords": ["symfony", "messenger", "enqueue"],
    "homepage": "http://symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Roze",
            "email": "samuel.roze@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        }
    ],
    "require": {
        "enqueue/enqueue-bundle": "^0.9",
        "symfony/messenger": "^4.3",
        "symfony/options-resolver": "^3.4|^4.2",
        "enqueue/amqp-tools": "^0.9"
    },
    "replace": {
        "enqueue/messenger-adapter": ">0.2.2"
    },
    "autoload": {
        "psr-4": { "Enqueue\\MessengerAdapter\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "symfony/yaml": "^3.4|^4.1"
    }
}
