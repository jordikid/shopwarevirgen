<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Shopware\Core\System\Snippet\Aggregate\SnippetSet\SnippetSetDefinition">
            <tag name="shopware.entity.definition" entity="snippet_set"/>
        </service>

        <service id="Shopware\Core\System\Snippet\SnippetDefinition">
            <tag name="shopware.entity.definition" entity="snippet"/>
        </service>
    </services>
</container>
